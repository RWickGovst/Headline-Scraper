<div class ="container">
    <h1>Tech News</h1>
    <div id="display-headlines">
        {{#each db_headlines}}
        {{#unless this.saved}}
        <div class="row">
            <div class = "col-10">
                <h3>{{this.title}}</h3>
                <h2>{{this.link}}</h2>
                <h4>{{this.summary}}</h4>
            </div>

        </div>
        {{/unless}}
        {{/each}}
    </div>
</div>

<footer id="footer">
<div class="card">
  <div class="card-header">
    Quote
  </div>
